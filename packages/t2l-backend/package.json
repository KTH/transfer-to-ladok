{
  "name": "t2l-backend",
  "version": "1.0.0",
  "description": "This is the \"backend\" side of Transfer to Ladok",
  "main": "src/index.ts",
  "types": "src/apiHandlers/utils/types.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn src/index.ts | pino-pretty -e options,response -o '{msg}' --ignore options,pid,hostname --singleLine --translateTime 'HH:MM:ss.l'",
    "start": "ts-node --transpile-only src/index.ts",
    "start:check": "ts-node --transpile-only src/check-certificate.ts | pino-pretty -o '{msg}' --ignore options,pid,hostname,app --singleLine --translateTime 'HH:MM:ss.l'",
    "test": "npm run test:tsc && npm run test:lint && npm run test:unit",
    "test:tsc": "tsc --noEmit",
    "test:lint": "prettier --check . && eslint src/",
    "test:unit": "jest",
    "test:watch": "jest --watch",
    "format": "prettier --write ."
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@kth/canvas-api": "^4.2.2",
    "@kth/reqvars": "^2.0.1",
    "applicationinsights": "^2.3.4",
    "connect-mongodb-session": "^3.1.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "got": "^11.8.3",
    "mongodb": "^4.8.1",
    "nanoid": "^3.3.4",
    "openid-client": "^5.1.5",
    "skog": "^3.0.2",
    "ts-node": "^10.8.1"
  },
  "devDependencies": {
    "@types/connect-mongodb-session": "^2.4.4",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.5",
    "@types/jest": "^28.1.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.28.0",
    "@typescript-eslint/parser": "^5.28.0",
    "eslint": "^8.17.0",
    "jest": "^28.1.3",
    "msw": "^0.44.2",
    "pino-pretty": "^7.6.1",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.3"
  }
}
