{
  "name": "t2l-backend",
  "version": "1.0.0",
  "description": "This is the \"backend\" side of Transfer to Ladok",
  "main": "src/index.ts",
  "scripts": {
    "dev": "ts-node-dev --respawn src/index.ts | pino-pretty -e options,response -o '{msg}' --ignore options,pid,hostname --singleLine --translateTime 'HH:MM:ss.l'",
    "start": "ts-node --transpile-only src/index.ts",
    "build:types": "tsc --emitDeclarationOnly",
    "test": "npm run test:tsc && npm run test:lint",
    "test:tsc": "tsc --noEmit src/**/*.ts",
    "test:lint": "prettier --check .",
    "format": "prettier --write ."
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@kth/canvas-api": "^4.2.2",
    "@kth/reqvars": "^2.0.1",
    "@types/express-session": "^1.17.4",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "got": "^11.8.3",
    "openid-client": "^5.1.5",
    "skog": "^3.0.2",
    "ts-node": "^10.7.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "pino-pretty": "^7.6.1",
    "prettier": "^2.7.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.7.3"
  }
}
